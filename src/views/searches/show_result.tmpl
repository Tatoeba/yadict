<%c++ #include "contents/searches.h" %>
<% skin %>
<% view searches_show_result uses contents::SearchesSimple extends master %>
<% template title() %><% gt "Search Result"  %><% end template %>

<% template main_content() %>
    <div class="module">
        <% c++ if(!content.whc.is_empty()) { %>
            <% c++ words_helper wh(out(), content.whc); %>
            <% c++ wh.words_list("/searches/show-result/" + content.queryStr + "/" + content.queryLang) ;%> 
        <% c++ } else { %>
            No Result
        <% c++ } %>
    </div>
<% end %>


<% end view %>
<% end skin %>
